{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "noImplicitReturns": false
  }
}
