{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb", "airbnb-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "import/prefer-default-export": "off",
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
